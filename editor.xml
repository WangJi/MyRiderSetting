<application>
  <component name="CodeInsightSettings">
    <option name="SHOW_PARAMETER_NAME_HINTS_ON_COMPLETION" value="true" />
    <option name="PARAMETER_INFO_DELAY" value="0" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="LINE_SPACING" value="1.1" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="JsonEditorOptions">
    <option name="COMMA_ON_ENTER" value="false" />
    <option name="COMMA_ON_MATCHING_BRACES" value="false" />
    <option name="COMMA_ON_PASTE" value="false" />
    <option name="AUTO_QUOTE_PROP_NAME" value="false" />
    <option name="AUTO_WHITESPACE_AFTER_COLON" value="false" />
    <option name="ESCAPE_PASTED_TEXT" value="false" />
  </component>
  <component name="LiveEditOptions" useJBChromeExtension="true" />
  <component name="SqlEditorOptions">
    <option name="aliasQualification" value="Always" />
    <option name="namespaceQualification" value="Always" />
    <option name="tableQualification" value="Always" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;note)(\W|$)(.*)" />
    <pattern pattern="\bNotImplementedException\b" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;hack)(\W|$)(.*)">
      <option name="FOREGROUND" value="fffdf9" />
      <option name="BACKGROUND" value="396dbb" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;critical)(\W|$)(.*)">
      <option name="FOREGROUND" value="ff8934" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;Important)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
  </component>
  <component name="YamlEditorOptions">
    <option name="useSmartPaste" value="false" />
  </component>
</application>